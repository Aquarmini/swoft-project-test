{
    "name": "limingxinleo/swoft-project",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "swoft"
    ],
    "description": "Modern High performance AOP and Coroutine PHP Framework, base on Swoole 2",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.0",
        "ext-swoole": ">=2.1",
        "swoft/component": "dev-feature",
        "limingxinleo/swoft-trait-instance": "^1.0",
        "limingxinleo/swoft-db-model": "^1.0",
        "limingxinleo/x-phalcon-enum": "^1.1",
        "limingxinleo/x-swoole-queue": "^1.2",
        "limingxinleo/swoft-aop-cacheable": "^1.1",
        "nikic/php-parser": "^4.0",
        "elasticsearch/elasticsearch": "^6.0",
        "topthink/think-validate": "^1.0",
        "limingxinleo/swoft-entity-cache": "^1.2",
        "limingxinleo/swoft-rpc": "^1.0"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Swoft.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Swoft\\Test\\": "test/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php bin/swoft app:init"
        ],
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    },
    "repositories": {
        "swoft-component": {
            "type": "git",
            "url": "https://github.com/limingxinleo/swoft-component.git"
        }
    }
}
